# Link to the kata: https://www.codewars.com/kata/5b077ebdaf15be5c7f000077/train/python
def count_sheep(n):
    if n < 1:
        return ''
    elif n == 1:
        return f"{n} sheep..."
    else:
        newstr = ''
        counter = 1
        for i in range(n):
            newstr += f"{counter} sheep..."
            counter += 1
        return newstr
